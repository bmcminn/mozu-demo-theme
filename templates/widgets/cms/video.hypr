
<div data-mz-widget="video-embed" data-mz-video-data="{{ model.config.url|default(model) }}">
  <noscript>Please enable JavaScript in order to load this Video.</noscript>
</div>

{% require_script "widgets/video" %}


<style type="text/css">
  [data-mz-widget="youtube-embed"] {
    display: block;
    width: 100%;
    opacity: 0;
  }
  [data-mz-widget="youtube-embed"].fade-in {
    opacity: 1;
    transition: opacity 0.2s linear;
  }

  .fade-in {
    opacity: 1;
  }
</style>
