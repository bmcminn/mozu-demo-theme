<!DOCTYPE html>
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0"/>

		{% if pageContext.isEditMode %}
			{% include "modules/json-required-for-hyprlive" %}
			{% include "modules/trailing-scripts" %}
			{% cms_resources %}
		{% endif %}

		<style type="text/css">{% inline_style "back-office.less" %}</style>
	</head>
	<body>

{# TODO: make this listen for a query parameter to be applied #}
{# {% if pageContext.isDebugMode %} #}
  <label for="dbg-model">model: <input id="dbg-model" type="text" value="{% json_attribute model %}"></label><br>
  <label for="dbg-order">order: <input id="dbg-order" type="text" value="{% json_attribute order %}"></label><br>
  <label for="dbg-pageContext">pageContext: <input id="dbg-pageContext" type="text" value="{% json_attribute pageContext %}"></label><br>
  <label for="dbg-siteContext">siteContext: <input id="dbg-siteContext" type="text" value="{% json_attribute siteContext %}"></label><br>
  <label for="dbg-user">user: <input id="dbg-user" type="text" value="{% json_attribute user %}"></label><br>

  {% preload_json model "model" %}
  {% preload_json order "order" %}
  {% preload_json pageContext "pageContext" %}
  {% preload_json siteContext "siteContext" %}
  {% preload_json user "user" %}
{# {% endif %} #}



		{% block body-content %}
		{% endblock body-content %}
	</body>
</html>
